# Copy this file to `.env` and fill in your real keys.
# NEVER commit your real `.env` file to git.

# ===== LLM keys =====
# Preferred: OpenAI API key
OPENAI_API_KEY=

# Fallback / alternative: OpenRouter API key
OPENROUTER_API_KEY=

# ===== JC settings GUI / launcher =====
JC_PROVIDER=openrouter
JC_OPENROUTER_MODEL=openai/gpt-4o-mini
JC_OPENAI_MODEL=gpt-4o-mini
JC_HUGGINGFACE_MODEL=meta-llama/llama-3.1-8b-instruct
JC_PORT=8000

# ===== API server runtime =====
API_HOST=127.0.0.1
API_PORT=8000
LOG_LEVEL=INFO

# ===== Security settings =====
# Generate a strong secret key with: python -c "import secrets; print(secrets.token_urlsafe(32))"
JC_SECRET_KEY=

# API key for authentication (optional - if not set, API runs without auth)
# Generate with: python -c "import secrets; print(secrets.token_urlsafe(32))"
JC_API_KEY=

# ===== Optional integrations =====
SERPER_API_KEY=
ELEVENLABS_API_KEY=
HUGGINGFACE_API_KEY=
GMAIL_CREDENTIALS=
NOTION_TOKEN=
SLACK_TOKEN=

# ===== Voice settings =====
ENABLE_VOICE=false
